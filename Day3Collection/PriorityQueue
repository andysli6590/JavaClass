package com.learnJava.Collections.PQ;

import java.util.PriorityQueue;

public class Solution7 {
    public static void main(String[] args) {
        PriorityQueue<Integer> pq1 = new PriorityQueue<>();
        pq1.add(1);
        pq1.add(2);
        pq1.add(3);
        pq1.add(5);
        pq1.add(8);
        pq1.add(16);
        System.out.println("the result of pq1 : " + pq1);
        PriorityQueue<Integer> pq2 = new PriorityQueue<>();
        pq2.add(1);
        pq2.add(2);
        pq2.add(6);
        pq2.add(5);
        pq2.add(9);
        pq2.add(16);
        System.out.println("the result of pq2 : " + pq2);
        for(Integer element : pq1){
            System.out.println(pq2.contains(element) ? "Yes" : "No");
        }

    }
}
