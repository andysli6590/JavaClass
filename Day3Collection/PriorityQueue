package com.learnJava.Collections.PQ;

import java.util.PriorityQueue;

public class Solution1 {
    public static void main(String[] args) {

        PriorityQueue pq = new PriorityQueue();

        pq.add("red");
        pq.add("white");
        pq.add("blue");

        System.out.println(pq);
    }
}

