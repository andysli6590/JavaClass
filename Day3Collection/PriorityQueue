package com.learnJava.Collections.PQ;

import java.util.PriorityQueue;

public class Solution3 {
    public static void main(String[] args) {
        PriorityQueue<String> pq = new PriorityQueue<>();
        pq.add("Red");
        pq.add("White");
        pq.add("Blue");
        System.out.println("the result of pq : " + pq);
        PriorityQueue<String> pq2 = new PriorityQueue<>();
        pq2.add("Green");
        pq2.add("Black");
        pq2.add("Yellow");
        System.out.println("the result of pq2" + pq2);

        pq.addAll(pq2);
        System.out.println("the result of new pq : " +pq);
    }
}

==================================================================================================================================

package com.learnJava.Collections.PQ;

import java.util.PriorityQueue;

public class Solution4 {
    public static void main(String[] args) {
        PriorityQueue<String> pq = new PriorityQueue<>();
        pq.add("red");
        pq.add("white");
        pq.add("blue");
        System.out.println("the result of original pq : " + pq);

        pq.offer("green");
        pq.offer("black");

        System.out.println("the result of new pq : " + pq);
    }
}

