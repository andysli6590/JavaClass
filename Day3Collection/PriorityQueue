package com.learnJava.Collections.PQ;

import java.util.PriorityQueue;

public class Solution11 {
    public static void main(String[] args) {
        PriorityQueue<Integer> pq1 = new PriorityQueue();
        pq1.add(1);
        pq1.add(2);
        pq1.add(3);
        pq1.add(4);
        pq1.add(5);
        pq1.add(6);
        System.out.println("the original result of pq1 : " + pq1);

        String s1 = pq1.toString();
        System.out.println("String representation of PQ: " + s1);
    }
}

