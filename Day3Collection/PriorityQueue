package com.learnJava.Collections.PQ;

import java.util.PriorityQueue;

public class Solution6 {
    public static void main(String[] args) {
        PriorityQueue<Integer> pq = new PriorityQueue();
        pq.add(1);
        pq.add(2);
        pq.add(3);
        System.out.println("the original result of pq: " + pq);
        System.out.println("the size of pq : " + pq.size());
    }
}
