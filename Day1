public class Java101112 {
    public static void main(String[] args) {
        int result1 = 23 / 10;
        System.out.println(result1);

        double result2 = 23.0 / 10;
        System.out.println(result2);

        double result3 = 23/10.0;
        System.out.println(result3);
    }
}

