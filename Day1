public class Java6 {

    /*
    * Write a Program to declare and initialize two double type variable.
    * Print the result of addition of these variables on the output screen
    * Hint: double input1 = 3.14;
    * double input2 = 5.0;
    * Output :
    * the sum of two variables = 8.14
    * */

    public static void main(String[] args) {
        double input1 = 3.14;
        double input2 = 5.0;
        double sum = input1 + input2;
        System.out.println(sum);

    }
}

package com.learnJava;

public class Java789 {
    /*
    * Write a Program to solve the expression :
    * -2 % 5 ;
    * store the result in a variable and print it on the output screen.
    * */
    public static void main(String[] args) {
        //7
        int result1 = -2 % 5;
        System.out.println(result1);
        //8
        int result2 = -2 % -5;
        System.out.println(result2);
        //9
        int result3 = 2 % -5;
        System.out.println(result3);
        
    }
}


