Output: input1 = 20;

input2 = 30;
    * */
    public static void main(String[] args) {
        int input1 = 20;
        int input2 = 30;
        System.out.println(input1);
        System.out.println(input2);
    }
}

package com.learnJava;

/*
* Write a Program to declare variables of all 8 primitive type, initialize and print them on output screen.
 *
* */


import org.w3c.dom.ls.LSOutput;

import java.util.jar.JarOutputStream;

public class Java2 {

    public static void main(String[] args) {


        //byte:1 signed byte (two's complement). Covers values from -128 to 127.
        byte aByte = 1;
        //shot: 2 bytes, signed (two's complement), -32,768 to 32,767
        short aShort = 2;
        //int: 4 bytes [-2^31, 2^31-1]
        int anInt = 3;
        //long: 8 bytes [-2^63, 2^63-1]
        long aLong = 400_000_000L;

        // float: 4 bytes, IEEE 754. Covers a range from 1.40129846432481707e-45 to 3.40282346638528860e+38
        // (positive or negative).
        float aFloat = 500_000_000F;

        //double: 8 bytes IEEE 754. Covers a range from 4.94065645841246544e-324d to 1.79769313486231570e+308d
        //(positive or negative).
        double aDouble = 456_989_324_987d;

        //char: 2 bytes, unsigned, Unicode, 0 to 65,535
        char aChar = 'A';

        //boolean: 1-bit. May take on the values true and false only.
        boolean isTrue = true;

        System.out.println(aByte);
        System.out.println(aShort);
        System.out.println(aLong);
        System.out.println(aFloat);
        System.out.println(aDouble);
        System.out.println(aChar);
        System.out.println(isTrue);


    }
    
