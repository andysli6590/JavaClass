public class Java13String {
    /*
    * Write a Program to declare variable to store your name (String name),
    * mobile no and email (String email).
    * Hint : use reference variable of String class for name and email.
    * String name = “Ravi”;
    * String email = “ravi@gla.ac.in”;
    *
    * */
    public static void main(String[] args) {
        String name = "ShenLi";
        String mobileNunber = "732-895-0843";
        String email = "shenli7530@gmail.com";

        System.out.println(name);
        System.out.println(mobileNunber);
        System.out.println(email);

    }
}

package Doc1ClassInterface;

public class Car {
    String model;
    int wheel;
    int gear;
    int speed;

    public Car(String model, int wheel, int gear, int speed){
        this.model = model;
        this.wheel = wheel;
        this.gear = gear;
        this.speed = speed;
    }

    public String getModel() {
        return model;
    }

    public void setModel(String model) {
        this.model = model;
    }

    public int getWheel() {
        return wheel;
    }

    public void setWheel(int wheel) {
        this.wheel = wheel;
    }

    public int getGear() {
        return gear;
    }

    public void setGear(int gear) {
        this.gear = gear;
    }

    public int getSpeed() {
        return speed;
    }

    public void setSpeed(int speed) {
        this.speed = speed;
    }

    @Override
    public String toString() {
        return "Car{" +
                "model='" + model + '\'' +
                ", wheel=" + wheel +
                ", gear=" + gear +
                ", speed=" + speed +
                '}';
    }
}

package Doc1ClassInterface;

public interface EVCars {
    void changeModel(int newValue);
    void changeWheel(int newValue);
    void changeGear(int newValue);
    void speedUp(int increment);
}

package Doc1ClassInterface;

public class Tesla {

    public static void main(String[] args) {
        Car tesla1 = new Car("X", 4,5,200);
        Car tesla2 = new Car("Y",4,6,250);
        Car tesla3 = new Car("S", 4, 7,260);

        System.out.println("tesla1 model:" + tesla1.model);
        System.out.println("tesla1 wheel:" + tesla1.wheel);
        System.out.println("tesla1 gear:" + tesla1.gear);
        System.out.println("tesla1 speed:" + tesla1.speed);
        System.out.println();

        System.out.println("tesla2 model:" + tesla2.model);
        System.out.println("tesla2 wheel:" + tesla2.wheel);
        System.out.println("tesla2 gear:" + tesla2.gear);
        System.out.println("tesla2 speed:" +tesla2.speed);
        System.out.println();

        System.out.println("tesla3 model:" +tesla3.model);
        System.out.println("tesla3 wheel:" +tesla3.wheel);
        System.out.println("tesla3 gear:" + tesla3.gear);
        System.out.println("tesla3 speed:" + tesla3.speed);

    }
}
